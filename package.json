{
  "name": "mythicsymbolic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "npm run build && gh-pages -d dist",
    "prettier:check": "prettier --check lib/**/*.ts lib/**/*.ts",
    "prettier:fix": "prettier --write   lib/**/*.ts lib/**/*.ts",
    "typecheck": "tsc --noemit",
    "test:watch": "jest --watch",
    "test": "jest",
    "build-prereqs": "node build-vocabulary.js && node build-avro-typescript.js",
    "build": "npm run build-prereqs && rimraf dist && parcel build index.html -d dist --public-url .",
    "watch": "npm run build-prereqs && parcel index.html -d dist-watch"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "testPathIgnorePatterns": [
      "dist",
      "dist-watch",
      ".cache"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@types/cheerio": "^0.22.23",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "avro-js": "^1.10.2",
    "avro-typescript": "^1.1.0",
    "babel-jest": "^26.6.3",
    "cheerio": "^1.0.0-rc.5",
    "classnames": "^2.3.1",
    "colorspaces": "^0.1.5",
    "gh-pages": "^3.1.0",
    "hsluv": "^0.1.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "toml": "^3.0.0",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
